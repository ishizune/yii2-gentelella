{
  "name": "ishizune/yii2-gentelella",
  "description": "Free admin template for backend",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "admin",
    "panel",
    "backend",
    "bootstrap3",
    "admin",
    "gentelella",
    "theme",
    "asset"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Laura Pagan",
      "email": "support@weblagoon.it",
      "homepage": "https://www.weblagoon.it",
      "role": "Creator"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "rmrevin/yii2-fontawesome": "*",
    "bower-asset/gentelella": "*"
  },
  "autoload": {
    "psr-4": {
      "ishizune\\gentelella\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
